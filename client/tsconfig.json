{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "lib": [
      "DOM",
      "dom.iterable",
      "ESNext"
    ],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["vite/client"]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
