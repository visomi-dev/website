root = true

[{*.go, *.raml}]
indent_style = tab
indent_size = 4
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true


[{*.json,*.raml,*.html,*.vue,*.ts,*.css,*.sass}]
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
